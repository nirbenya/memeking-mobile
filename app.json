{
  "expo": {
    "name": "Memeking",
    "slug": "memeking-webview",
    "version": "3.1.0",
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#5f4afb"
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow memeking to access your photos.",
          "savePhotosPermission": "Allow memeking to save photos.",
          "isAccessMediaLocationEnabled": "true"
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow Memeking to access your photos.",
        "NSPhotoLibraryAddUsageDescription": "Allow Memeking to save photos."
      }
    },
    "android": {
      "versionCode": 4,
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icon.png",
        "backgroundColor": "#5f4afb"
      },
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "true"
      ],
      "package": "memeking.exp.component"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
